# Author:  Denis Vakatov (vakatov@ncbi.nlm.nih.gov)
#################################
# Meta-makefile(to be configured into "Makefile")
# This can be used to build/install/clean [*_PROJ] makefiles
# in current directory and in the below [SUB_PROJ] directories.
#
# LIB_PROJ = pl1 pl2 ...  // Makefile.pl1.lib Makefile.pl2.lib ...
# APP_PROJ = pa1 pa2 ...  // Makefile.pa1.app Makefile.pa2.app ...
# USR_PROJ = pu1 pu2 ...  // Makefile.pu1 Makefile.pu2 ...
# SUB_PROJ = app sub_proj1 sub_proj2
#
# srcdir = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST/src/build-system
# include /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST/ReleaseMT/build/Makefile.meta
#################################

builddir = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST/ReleaseMT/build
build_root = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST/ReleaseMT
top_srcdir = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST
abs_top_srcdir = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST
rel_srcdir = $(srcdir:$(top_srcdir)/%=%)
abs_srcdir = $(srcdir:$(top_srcdir)/%=$(abs_top_srcdir)/%)
subdir     = $(rel_srcdir:src/%=%)
status_dir = /home/msamanta/Electrophorus-Toolbox/NCBI-BLAST/ReleaseMT/status

### Make sure we are using a right command shell

SHELL=/bin/bash
RM    = /bin/rm
TOUCH = /bin/touch
MKDIR = /bin/mkdir

signature      = GCC_470-ReleaseMT64--x86_64-unknown-linux3.2.0-gnu2.13-samson
XUNIX_USR_PROJ = $(UNIX_USR_PROJ)
APP_NOCOPY     = 

CPP       = /usr/bin/gcc  -std=gnu11 -fgnu89-inline -E
LN_S      = /bin/ln -s
PROJECTS_ = 
DEPFLAGS  = -M
DEPFLAGS_POST = 
obj_ext   = .o
GREP      = /bin/grep
EGREP     = /bin/grep -E
EGREP_Q   = /bin/grep -E -q
SED       = /bin/sed
OSTYPE    = linux

deactivate = unlink
 deactivate = deactivate

default_rule: all

include $(top_srcdir)/src/build-system/Makefile.is_gmake
include $(top_srcdir)/src/build-system/Makefile.meta.$(is_gmake)

configurables = configurables
include $(top_srcdir)/src/build-system/Makefile.$(configurables)
