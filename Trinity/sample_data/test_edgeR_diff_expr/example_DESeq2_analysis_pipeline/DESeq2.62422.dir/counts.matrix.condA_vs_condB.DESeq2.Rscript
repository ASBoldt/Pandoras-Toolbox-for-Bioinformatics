library(DESeq2)

data = read.table("/ahg/regev/users/bhaas/seq/bhaas/SVN/trinityrnaseq/trunk/sample_data/test_edgeR_diff_expr/example_DESeq2_analysis_pipeline/counts.matrix", header=T, row.names=1, com='')
col_ordering = c(1,2,3,4,5,6,7,8,9)
rnaseqMatrix = data[,col_ordering]
rnaseqMatrix = round(rnaseqMatrix)
rnaseqMatrix = rnaseqMatrix[rowSums(rnaseqMatrix)>=2,]
conditions = data.frame(conditions=factor(c(rep("condA", 5), rep("condB", 4))))
rownames(conditions) = colnames(rnaseqMatrix)
ddsFullCountTable <- DESeqDataSetFromMatrix(
    countData = rnaseqMatrix,
    colData = conditions,
    design = ~ conditions)
dds = DESeq(ddsFullCountTable)
res = results(dds)

write.table(res[order(res$pvalue),], file='counts.matrix.condA_vs_condB.DESeq2.DE_results', sep='	', quote=FALSE, row.names=T)
source("/ahg/regev/users/bhaas/seq/bhaas/SVN/trinityrnaseq/trunk/Analysis/DifferentialExpression/R/rnaseq_plot_funcs.R")
pdf("counts.matrix.condA_vs_condB.DESeq2.DE_results.MA_n_Volcano.pdf")
plot_MA_and_Volcano(log2(res$baseMean+1), res$log2FoldChange, res$padj)
dev.off()
